{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "FILESYSTEMBACKEND_VERSION": false
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "no-console": [0],
    "react-hooks/rules-of-hooks": "error"
  }
}
