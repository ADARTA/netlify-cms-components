{
  "name": "netlify-cms-backend-fs",
  "version": "0.0.0-development",
  "description": "Backend for Netlify CMS to test with local file system",
  "repository": {
    "type": "git",
    "url": "https://github.com/ADARTA/netlify-cms-components.git"
  },
  "main": "dist/fs-backend.js",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "test:simple": "node ./scripts/_tests_/simple.js",
    "build:copy": "cp-cli scripts/fs/ dist/fs/",
    "build:lib": "webpack-cli --mode=production",
    "build:dev": "webpack-cli --mode=development",
    "build": "run-s build:copy build:lib",
    "dev": "run-s build:copy build:dev",
    "format": "prettier --write \"src/**/*.js\"",
    "prepublishOnly": "echo \"Create test and lint - (npm run test && npm run lint)\"",
    "version": "run-s format",
    "semantic-release:local": "semantic-release pre --debug=false && npm publish && semantic-release post --debug=false",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "js-base64": "^2.5.0",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "cp-cli": "^1.1.2",
    "eslint": "5.13.0",
    "eslint-plugin-react-hooks": "1.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "4.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "react": "^16.8.1"
  },
  "keywords": [
    "adarta",
    "netlify-cms",
    "backend",
    "fs"
  ],
  "author": "talves",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Opera versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
